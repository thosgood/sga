    <fr:p>This is a simple translation of <fr:ref
addr="sga1-i.7.7"
href="sga1-i.7.7.xml"
taxon="Corollary"
number="I.7.7" />.</fr:p>
  </fr:mainmatter><fr:backmatter /></fr:tree>
  
